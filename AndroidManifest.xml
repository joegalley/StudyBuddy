<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.atthack.studybuddy"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.NFC" />

    <uses-feature
        android:name="android.hardware.NFC"
        android:required="True" />

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/sb_launcher2"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.atthack.studybuddy.LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.atthack.studybuddy.Main" >
        </activity>
        <activity android:name="com.atthack.studybuddy.MenuActivity" />
        <activity android:name="com.atthack.studybuddy.StudyingAroundMeActivity" />
        <activity android:name="com.atthack.studybuddy.SeeOpenTablesActivity" />
        <activity android:name="com.atthack.studybuddy.NFCReader" />
    </application>

</manifest>